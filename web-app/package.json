{
  "name": "user-management",
  "version": "1.0.0",
  "description": "React web application hosted from a C++ web server",
  "main": "index.js",
  "author": "Chris Mc",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --env.env=dev --env.API_URL=https://localhost:8080",
    "dev:bundleanalyzer": "yarn dev -- --env.addons=bundleanalyzer",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack -p --env.env=prod --env.API_URL=https://ec2-3-14-64-10.us-east-2.compute.amazonaws.com/",
    "build:bundleanalyzer": "yarn build -- --env.addons=bundleanalyzer",
    "lint": "eslint --fix src/*.js src/components/*.js src/components/**/*.js",
    "test": "NODE_ENV=dev API_URL=http://localhost:3001 jest",
    "coverage": "yarn test --coverage"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-async": "^10.0.1",
    "react-delay-render": "^0.1.2",
    "react-dom": "^16.8.6",
    "react-imported-component": "^5.5.3",
    "react-live-clock": "^4.0.5",
    "react-router-dom": "^5.0.1",
    "semantic-ui-react": "^0.87.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/dom": "^7.21.6",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.0.17",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-terminator": "^2.0.3",
    "jest": "^26.1.0",
    "jest-fetch-mock": "^3.0.3",
    "mini-css-extract-plugin": "^0.7.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react-hot-loader": "^4.12.6",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "wait-for-expect": "^3.0.2",
    "webpack": "^4.35.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "keywords": [
    "react",
    "react 16",
    "boilerplate",
    "semantic ui",
    "semantic ui react",
    "webpack 4",
    "webpack",
    "hot module replacement",
    "hmr"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!/node_modules/"
    ]
  }
}
